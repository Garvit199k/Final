{
  "name": "typing-test-game",
  "version": "1.0.0",
  "description": "An interactive typing test application with theme customization",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "preview": "live-server public --port=8080 --no-browser",
    "dev:server": "nodemon server.js",
    "dev:client": "live-server public --port=8080 --no-browser",
    "dev:all": "concurrently \"npm run dev:server\" \"npm run dev:client\""
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.18.3",
    "jsonwebtoken": "^9.0.2"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "live-server": "^1.2.2",
    "nodemon": "^3.1.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
